{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/avatar.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=2592000"
        }
      ]
    }
  ]
}